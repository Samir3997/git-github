package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import java.math.BigDecimal;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag50Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag50 block4Tag50 = new Block4Tag50();

        // Set properties
        block4Tag50.setBlock4Tag50aAcct("account123");
        block4Tag50.setBlock4Tag50aBic("BIC123");
        block4Tag50.setBlock4Tag50fPartyId("party123");
        block4Tag50.setBlock4Tag50fNumber1("12345");
        block4Tag50.setBlock4Tag50fNameAddr1("Name Address 1");
        block4Tag50.setBlock4Tag50fNumber2("67890");
        block4Tag50.setBlock4Tag50fNameAddr2("Name Address 2");
        block4Tag50.setBlock4Tag50fNumber3("112233");
        block4Tag50.setBlock4Tag50fNameAddr3("Name Address 3");
        block4Tag50.setBlock4Tag50fNumber4("445566");
        block4Tag50.setBlock4Tag50fNameAddr4("Name Address 4");
        block4Tag50.setBlock4Tag50kAccount("kAccount123");
        block4Tag50.setBlock4Tag50kNameAddr("K Name Address");

        // Assert properties
        assertEquals("account123", block4Tag50.getBlock4Tag50aAcct());
        assertEquals("BIC123", block4Tag50.getBlock4Tag50aBic());
        assertEquals("party123", block4Tag50.getBlock4Tag50fPartyId());
        assertEquals("12345", block4Tag50.getBlock4Tag50fNumber1());
        assertEquals("Name Address 1", block4Tag50.getBlock4Tag50fNameAddr1());
        assertEquals("67890", block4Tag50.getBlock4Tag50fNumber2());
        assertEquals("Name Address 2", block4Tag50.getBlock4Tag50fNameAddr2());
        assertEquals("112233", block4Tag50.getBlock4Tag50fNumber3());
        assertEquals("Name Address 3", block4Tag50.getBlock4Tag50fNameAddr3());
        assertEquals("445566", block4Tag50.getBlock4Tag50fNumber4());
        assertEquals("Name Address 4", block4Tag50.getBlock4Tag50fNameAddr4());
        assertEquals("kAccount123", block4Tag50.getBlock4Tag50kAccount());
        assertEquals("K Name Address", block4Tag50.getBlock4Tag50kNameAddr());
    }
}
