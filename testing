import static org.junit.jupiter.api.Assertions.assertEquals;

import java.math.BigDecimal;

import org.junit.jupiter.api.Test;

public class MT940Block4Test {

    @Test
    public void testMT940Block4() {
        // Create an instance of MT940Block4
        MT940Block4 block4 = new MT940Block4();

        // Set values
        block4.setBlock4Tag20("TAG20Value");
        block4.setBlock4Tag21("TAG21Value");
        block4.setBlock4Tag25("TAG25Value");
        block4.setBlock4Tag28CStatNum("STATNumValue");
        block4.setBlock4Tag28CSeqNum("SEQNumValue");
        block4.setBlock4Tag25PAcct("AccountValue");
        block4.setBlock4Tag25PBIC("BICValue");
        block4.setBlock4Tag64DCMark("DCMarkValue");
        block4.setBlock4Tag64Date("2023-01-01");
        block4.setBlock4Tag64Currency("USD");
        block4.setBlock4Tag64CloseAvailBal(new BigDecimal("5000.00"));

        // Assert values
        assertEquals("TAG20Value", block4.getBlock4Tag20());
        assertEquals("TAG21Value", block4.getBlock4Tag21());
        assertEquals("TAG25Value", block4.getBlock4Tag25());
        assertEquals("STATNumValue", block4.getBlock4Tag28CStatNum());
        assertEquals("SEQNumValue", block4.getBlock4Tag28CSeqNum());
        assertEquals("AccountValue", block4.getBlock4Tag25PAcct());
        assertEquals("BICValue", block4.getBlock4Tag25PBIC());
        assertEquals("DCMarkValue", block4.getBlock4Tag64DCMark());
        assertEquals("2023-01-01", block4.getBlock4Tag64Date());
        assertEquals("USD", block4.getBlock4Tag64Currency());
        assertEquals(new BigDecimal("5000.00"), block4.getBlock4Tag64CloseAvailBal());
    }
}
