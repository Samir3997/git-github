package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import java.math.BigDecimal;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag60Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag60 block4Tag60 = new Block4Tag60();

        // Set properties
        block4Tag60.setBlock4Tag60FdCMark("DCMarkF");
        block4Tag60.setBlock4Tag60FDate("2024-01-01");
        block4Tag60.setBlock4Tag60FCurrency("USD");
        block4Tag60.setBlock4Tag60FAmnt(new BigDecimal("1000.00"));
        block4Tag60.setBlock4Tag60MdCMark("DCMarkM");
        block4Tag60.setBlock4Tag60MDate("2024-01-02");
        block4Tag60.setBlock4Tag60MCurrency("EUR");
        block4Tag60.setBlock4Tag60MAmnt(new BigDecimal("2000.00"));

        // Assert properties
        assertEquals("DCMarkF", block4Tag60.getBlock4Tag60FdCMark());
        assertEquals("2024-01-01", block4Tag60.getBlock4Tag60FDate());
        assertEquals("USD", block4Tag60.getBlock4Tag60FCurrency());
        assertEquals(new BigDecimal("1000.00"), block4Tag60.getBlock4Tag60FAmnt());
        assertEquals("DCMarkM", block4Tag60.getBlock4Tag60MdCMark());
        assertEquals("2024-01-02", block4Tag60.getBlock4Tag60MDate());
        assertEquals("EUR", block4Tag60.getBlock4Tag60MCurrency());
        assertEquals(new BigDecimal("2000.00"), block4Tag60.getBlock4Tag60MAmnt());
    }
}
