package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag77Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag77 block4Tag77 = new Block4Tag77();

        // Set properties
        block4Tag77.setBlock4Tag77bCode("Code123");
        block4Tag77.setBlock4Tag77bCountryCode("US");
        block4Tag77.setBlock4Tag77bNarrative("This is a narrative.");

        // Assert properties
        assertEquals("Code123", block4Tag77.getBlock4Tag77bCode());
        assertEquals("US", block4Tag77.getBlock4Tag77bCountryCode());
        assertEquals("This is a narrative.", block4Tag77.getBlock4Tag77bNarrative());
    }
}
