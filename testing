package com.maybank.ilm.embeddable;

import java.math.BigDecimal;

import javax.persistence.Column;
import javax.persistence.Embeddable;

@Embeddable
public class Block4Tag99B {

	@Column(name = "V_BLOCK4_TAG_99B_QUALIFIER") 
	private String block4Tag99BQualifier; 

	@Column(name = "N_BLOCK4_TAG_99B_NUMBER") 
	private BigDecimal block4Tag99BNumber;

	public String getBlock4Tag99BQualifier() {
		return block4Tag99BQualifier;
	}

	public void setBlock4Tag99BQualifier(String block4Tag99BQualifier) {
		this.block4Tag99BQualifier = block4Tag99BQualifier;
	}

	public BigDecimal getBlock4Tag99BNumber() {
		return block4Tag99BNumber;
	}

	public void setBlock4Tag99BNumber(BigDecimal block4Tag99BNumber) {
		this.block4Tag99BNumber = block4Tag99BNumber;
	} 
}
