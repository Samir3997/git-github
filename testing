package com.maybank.ilm.embeddable;

import java.io.Serializable;
import java.math.BigDecimal;

import javax.persistence.Column;
import javax.persistence.Embeddable;

@Embeddable
public class MT202Block4 implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -3402887972982015083L;

	@Column(name = "V_BLOCK4_TAG_20") 
	private String block4Tag20; 
	
	@Column(name = "V_BLOCK4_TAG_21") 
	private String block4Tag21;
	
	@Column(name = "V_BLOCK4_TAG_32A_DATE") 
	private String block4Tag32ADate;
	
	@Column(name = "V_BLOCK4_TAG_32A_CURR") 
	private String block4Tag32ACurr;
	
	@Column(name = "N_BLOCK4_TAG_32A_AMNT") 
	private BigDecimal block4Tag32AAmnt;

	public String getBlock4Tag20() {
		return block4Tag20;
	}

	public void setBlock4Tag20(String block4Tag20) {
		this.block4Tag20 = block4Tag20;
	}

	public String getBlock4Tag21() {
		return block4Tag21;
	}

	public void setBlock4Tag21(String block4Tag21) {
		this.block4Tag21 = block4Tag21;
	}

	public String getBlock4Tag32ADate() {
		return block4Tag32ADate;
	}

	public void setBlock4Tag32ADate(String block4Tag32ADate) {
		this.block4Tag32ADate = block4Tag32ADate;
	}

	public String getBlock4Tag32ACurr() {
		return block4Tag32ACurr;
	}

	public void setBlock4Tag32ACurr(String block4Tag32ACurr) {
		this.block4Tag32ACurr = block4Tag32ACurr;
	}

	public BigDecimal getBlock4Tag32AAmnt() {
		return block4Tag32AAmnt;
	}

	public void setBlock4Tag32AAmnt(BigDecimal block4Tag32AAmnt) {
		this.block4Tag32AAmnt = block4Tag32AAmnt;
	}
}
