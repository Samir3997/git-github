package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import java.math.BigDecimal;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag92BTest {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag92B block4Tag92B = new Block4Tag92B();

        // Set properties
        block4Tag92B.setBlock4Tag92BQualifier("QualifierB");
        block4Tag92B.setBlock4Tag92BFrstCurCode("USD");
        block4Tag92B.setBlock4Tag92BScndCurCode("EUR");
        block4Tag92B.setBlock4Tag92BRate(BigDecimal.valueOf(1.5));

        // Assert properties
        assertEquals("QualifierB", block4Tag92B.getBlock4Tag92BQualifier());
        assertEquals("USD", block4Tag92B.getBlock4Tag92BFrstCurCode());
        assertEquals("EUR", block4Tag92B.getBlock4Tag92BScndCurCode());
        assertEquals(BigDecimal.valueOf(1.5), block4Tag92B.getBlock4Tag92BRate());
    }
}
