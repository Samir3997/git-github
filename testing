import static org.junit.jupiter.api.Assertions.assertEquals;

import java.math.BigDecimal;

import org.junit.jupiter.api.Test;

public class MT200Block4Test {

    @Test
    public void testMT200Block4() {
        // Create an instance of MT200Block4
        MT200Block4 block4 = new MT200Block4();

        // Set values
        block4.setBlock4Tag20("TAG20Value");
        block4.setBlock4Tag32ADate("2023-01-01");
        block4.setBlock4Tag32ACurr("USD");
        block4.setBlock4Tag32AAmnt(new BigDecimal("1500.00"));

        // Assert values
        assertEquals("TAG20Value", block4.getBlock4Tag20());
        assertEquals("2023-01-01", block4.getBlock4Tag32ADate());
        assertEquals("USD", block4.getBlock4Tag32ACurr());
        assertEquals(new BigDecimal("1500.00"), block4.getBlock4Tag32AAmnt());
    }
}
