package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag53Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag53 block4Tag53 = new Block4Tag53();

        // Set properties
        block4Tag53.setBlock4Tag53aDcmark("DCMarkA");
        block4Tag53.setBlock4Tag53aAcct("accountA");
        block4Tag53.setBlock4Tag53aBic("BIC_A");
        block4Tag53.setBlock4Tag53bDcmark("DCMarkB");
        block4Tag53.setBlock4Tag53bAcct("accountB");
        block4Tag53.setBlock4Tag53bLocation("Location B");
        block4Tag53.setBlock4Tag53dDcmark("DCMarkD");
        block4Tag53.setBlock4Tag53dAcct("accountD");
        block4Tag53.setBlock4Tag53dNameAddr("Name Address D");

        // Assert properties
        assertEquals("DCMarkA", block4Tag53.getBlock4Tag53aDcmark());
        assertEquals("accountA", block4Tag53.getBlock4Tag53aAcct());
        assertEquals("BIC_A", block4Tag53.getBlock4Tag53aBic());
        assertEquals("DCMarkB", block4Tag53.getBlock4Tag53bDcmark());
        assertEquals("accountB", block4Tag53.getBlock4Tag53bAcct());
        assertEquals("Location B", block4Tag53.getBlock4Tag53bLocation());
        assertEquals("DCMarkD", block4Tag53.getBlock4Tag53dDcmark());
        assertEquals("accountD", block4Tag53.getBlock4Tag53dAcct());
        assertEquals("Name Address D", block4Tag53.getBlock4Tag53dNameAddr());
    }
}
