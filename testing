package com.maybank.ilm.embeddable;

import java.math.BigDecimal;

import javax.persistence.Column;
import javax.persistence.Embeddable;

@Embeddable
public class Block4Tag99A {

	@Column(name = "V_BLOCK4_TAG_99A_QUALIFIER") 
	private String block4Tag99AQualifier; 

	@Column(name = "V_BLOCK4_TAG_99A_SIGN") 
	private String block4Tag99ASign; 

	@Column(name = "N_BLOCK4_TAG_99A_AMOUNT") 
	private BigDecimal block4Tag99AAmount;

	public String getBlock4Tag99AQualifier() {
		return block4Tag99AQualifier;
	}

	public void setBlock4Tag99AQualifier(String block4Tag99AQualifier) {
		this.block4Tag99AQualifier = block4Tag99AQualifier;
	}

	public String getBlock4Tag99ASign() {
		return block4Tag99ASign;
	}

	public void setBlock4Tag99ASign(String block4Tag99ASign) {
		this.block4Tag99ASign = block4Tag99ASign;
	}

	public BigDecimal getBlock4Tag99AAmount() {
		return block4Tag99AAmount;
	}

	public void setBlock4Tag99AAmount(BigDecimal block4Tag99AAmount) {
		this.block4Tag99AAmount = block4Tag99AAmount;
	}
}
