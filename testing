package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag54Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag54 block4Tag54 = new Block4Tag54();

        // Set properties
        block4Tag54.setBlock4Tag54aDcmark("DCMarkA");
        block4Tag54.setBlock4Tag54aAcct("accountA");
        block4Tag54.setBlock4Tag54aBic("BIC_A");
        block4Tag54.setBlock4Tag54bDcmark("DCMarkB");
        block4Tag54.setBlock4Tag54bAcct("accountB");
        block4Tag54.setBlock4Tag54bLocation("Location B");
        block4Tag54.setBlock4Tag54dDcmark("DCMarkD");
        block4Tag54.setBlock4Tag54dAcct("accountD");
        block4Tag54.setBlock4Tag54dNameAddr("Name Address D");

        // Assert properties
        assertEquals("DCMarkA", block4Tag54.getBlock4Tag54aDcmark());
        assertEquals("accountA", block4Tag54.getBlock4Tag54aAcct());
        assertEquals("BIC_A", block4Tag54.getBlock4Tag54aBic());
        assertEquals("DCMarkB", block4Tag54.getBlock4Tag54bDcmark());
        assertEquals("accountB", block4Tag54.getBlock4Tag54bAcct());
        assertEquals("Location B", block4Tag54.getBlock4Tag54bLocation());
        assertEquals("DCMarkD", block4Tag54.getBlock4Tag54dDcmark());
        assertEquals("accountD", block4Tag54.getBlock4Tag54dAcct());
        assertEquals("Name Address D", block4Tag54.getBlock4Tag54dNameAddr());
    }
}
