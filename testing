import org.junit.jupiter.api.Test;
import org.junit.jupiter.api.assertThrows;
import com.maybank.ilm.utility.exception.ILMException;
import com.maybank.ilm.core.service.MXMsgHandlerAbstractImpl;

class MXMsgHandlerAbstractImplTest {

    @Test
    void testThrowEntityNotFountException() {
        MXMsgHandlerAbstractImpl handler = new MXMsgHandlerAbstractImpl();
        assertThrows(ILMException.class, () -> handler.throwEntityNotFountException());
    }
}
