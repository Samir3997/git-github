package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag52Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag52 block4Tag52 = new Block4Tag52();

        // Set properties
        block4Tag52.setBlock4Tag52aDcmark("DCMarkA");
        block4Tag52.setBlock4Tag52aAcct("accountA");
        block4Tag52.setBlock4Tag52aBic("BIC_A");
        block4Tag52.setBlock4Tag52dDcmark("DCMarkD");
        block4Tag52.setBlock4Tag52dAcct("accountD");
        block4Tag52.setBlock4Tag52dNameAddr("Name Address D");

        // Assert properties
        assertEquals("DCMarkA", block4Tag52.getBlock4Tag52aDcmark());
        assertEquals("accountA", block4Tag52.getBlock4Tag52aAcct());
        assertEquals("BIC_A", block4Tag52.getBlock4Tag52aBic());
        assertEquals("DCMarkD", block4Tag52.getBlock4Tag52dDcmark());
        assertEquals("accountD", block4Tag52.getBlock4Tag52dAcct());
        assertEquals("Name Address D", block4Tag52.getBlock4Tag52dNameAddr());
    }
}
