import static org.junit.jupiter.api.Assertions.assertEquals;

import java.math.BigDecimal;

import org.junit.jupiter.api.Test;

public class MT900MT910Block4Test {

    @Test
    public void testMT900MT910Block4() {
        // Create an instance of MT900MT910Block4
        MT900MT910Block4 block4 = new MT900MT910Block4();

        // Set values
        block4.setBlock4Tag20("TAG20Value");
        block4.setBlock4Tag21("TAG21Value");
        block4.setBlock4Tag25PAcct("AccountValue");
        block4.setBlock4Tag25PBic("BICValue");
        block4.setBlock4Tag25("TAG25Value");
        block4.setBlock4Tag32ADate("2023-01-01");
        block4.setBlock4Tag32ACurr("USD");
        block4.setBlock4Tag32AAmnt(new BigDecimal("1500.00"));

        // Assert values
        assertEquals("TAG20Value", block4.getBlock4Tag20());
        assertEquals("TAG21Value", block4.getBlock4Tag21());
        assertEquals("AccountValue", block4.getBlock4Tag25PAcct());
        assertEquals("BICValue", block4.getBlock4Tag25PBic());
        assertEquals("TAG25Value", block4.getBlock4Tag25());
        assertEquals("2023-01-01", block4.getBlock4Tag32ADate());
        assertEquals("USD", block4.getBlock4Tag32ACurr());
        assertEquals(new BigDecimal("1500.00"), block4.getBlock4Tag32AAmnt());
    }
}
