import static org.junit.jupiter.api.Assertions.assertEquals;

import java.math.BigDecimal;

import org.junit.jupiter.api.Test;

public class MT400Block4Test {

    @Test
    public void testMT400Block4() {
        // Create an instance of MT400Block4
        MT400Block4 block4 = new MT400Block4();

        // Set values
        block4.setBlock4Tag20("TAG20Value");
        block4.setBlock4Tag21("TAG21Value");
        block4.setBlock4Tag33a(new BigDecimal("3000.00"));
        block4.setBlock4Tag71b("TAG71BValue");
        block4.setBlock4Tag73("TAG73Value");

        // Assert values
        assertEquals("TAG20Value", block4.getBlock4Tag20());
        assertEquals("TAG21Value", block4.getBlock4Tag21());
        assertEquals(new BigDecimal("3000.00"), block4.getBlock4Tag33a());
        assertEquals("TAG71BValue", block4.getBlock4Tag71b());
        assertEquals("TAG73Value", block4.getBlock4Tag73());
    }
}
