package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import java.math.BigDecimal;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag98ETest {

    @Test
    public void testBlock4Tag98E() {
        // Creating a new Block4Tag98E object
        Block4Tag98E block4Tag98E = new Block4Tag98E();
        
        // Setting values
        block4Tag98E.setBlock4Tag98EQualifier("QualifierE");
        block4Tag98E.setBlock4Tag98EDate("2023-11-01");
        block4Tag98E.setBlock4Tag98ETime("12:30:00");
        block4Tag98E.setBlock4Tag98EDecimals(new BigDecimal("123.45"));
        block4Tag98E.setBlock4Tag98ESign("+");
        block4Tag98E.setBlock4Tag98EUtcInd("Y");

        // Asserting values
        assertEquals("QualifierE", block4Tag98E.getBlock4Tag98EQualifier());
        assertEquals("2023-11-01", block4Tag98E.getBlock4Tag98EDate());
        assertEquals("12:30:00", block4Tag98E.getBlock4Tag98ETime());
        assertEquals(new BigDecimal("123.45"), block4Tag98E.getBlock4Tag98EDecimals());
        assertEquals("+", block4Tag98E.getBlock4Tag98ESign());
        assertEquals("Y", block4Tag98E.getBlock4Tag98EUtcInd());
    }
}
