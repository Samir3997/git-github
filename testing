package com.maybank.ilm.embeddable;

import java.math.BigDecimal;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag70to71Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag70to71 block4Tag70to71 = new Block4Tag70to71();

        // Set properties
        block4Tag70to71.setBlock4Tag70Nar1("Narrative 1");
        block4Tag70to71.setBlock4Tag70Nar2("Narrative 2");
        block4Tag70to71.setBlock4Tag70Nar3("Narrative 3");
        block4Tag70to71.setBlock4Tag70Nar4("Narrative 4");
        block4Tag70to71.setBlock4Tag71a("Tag 71A");
        block4Tag70to71.setBlock4Tag71gCurrency("USD");
        block4Tag70to71.setBlock4Tag71gReceivCharge(BigDecimal.valueOf(100.50));

        // Assert properties
        assertEquals("Narrative 1", block4Tag70to71.getBlock4Tag70Nar1());
        assertEquals("Narrative 2", block4Tag70to71.getBlock4Tag70Nar2());
        assertEquals("Narrative 3", block4Tag70to71.getBlock4Tag70Nar3());
        assertEquals("Narrative 4", block4Tag70to71.getBlock4Tag70Nar4());
        assertEquals("Tag 71A", block4Tag70to71.getBlock4Tag71a());
        assertEquals("USD", block4Tag70to71.getBlock4Tag71gCurrency());
        assertEquals(BigDecimal.valueOf(100.50), block4Tag70to71.getBlock4Tag71gReceivCharge());
    }
}
