package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import java.math.BigDecimal;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag62Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag62 block4Tag62 = new Block4Tag62();

        // Set properties
        block4Tag62.setBlock4Tag62FdCMark("DCMarkF");
        block4Tag62.setBlock4Tag62FDate("2024-01-01");
        block4Tag62.setBlock4Tag62FCurrency("USD");
        block4Tag62.setBlock4Tag62FAmnt(new BigDecimal("1000.00"));
        block4Tag62.setBlock4Tag62MdCMark("DCMarkM");
        block4Tag62.setBlock4Tag62MDate("2024-01-02");
        block4Tag62.setBlock4Tag62MCurrency("EUR");
        block4Tag62.setBlock4Tag62MAmnt(new BigDecimal("2000.00"));

        // Assert properties
        assertEquals("DCMarkF", block4Tag62.getBlock4Tag62FdCMark());
        assertEquals("2024-01-01", block4Tag62.getBlock4Tag62FDate());
        assertEquals("USD", block4Tag62.getBlock4Tag62FCurrency());
        assertEquals(new BigDecimal("1000.00"), block4Tag62.getBlock4Tag62FAmnt());
        assertEquals("DCMarkM", block4Tag62.getBlock4Tag62MdCMark());
        assertEquals("2024-01-02", block4Tag62.getBlock4Tag62MDate());
        assertEquals("EUR", block4Tag62.getBlock4Tag62MCurrency());
        assertEquals(new BigDecimal("2000.00"), block4Tag62.getBlock4Tag62MAmnt());
    }
}
