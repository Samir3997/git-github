package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag72Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag72 block4Tag72 = new Block4Tag72();

        // Set properties
        block4Tag72.setBlock4Tag72Nar1("Narrative 1");
        block4Tag72.setBlock4Tag72Nar2("Narrative 2");
        block4Tag72.setBlock4Tag72Nar3("Narrative 3");
        block4Tag72.setBlock4Tag72Nar4("Narrative 4");
        block4Tag72.setBlock4Tag72Nar5("Narrative 5");
        block4Tag72.setBlock4Tag72Nar6("Narrative 6");

        // Assert properties
        assertEquals("Narrative 1", block4Tag72.getBlock4Tag72Nar1());
        assertEquals("Narrative 2", block4Tag72.getBlock4Tag72Nar2());
        assertEquals("Narrative 3", block4Tag72.getBlock4Tag72Nar3());
        assertEquals("Narrative 4", block4Tag72.getBlock4Tag72Nar4());
        assertEquals("Narrative 5", block4Tag72.getBlock4Tag72Nar5());
        assertEquals("Narrative 6", block4Tag72.getBlock4Tag72Nar6());
    }
}
