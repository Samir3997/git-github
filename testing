package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class MT019Block4Test {

    @Test
    public void testMT019Block4() {
        // Creating a new MT019Block4 object
        MT019Block4 mt019Block4 = new MT019Block4();

        // Setting values
        mt019Block4.setBlock4175InputTime("10:30");
        mt019Block4.setBlock4106Mir("123456");
        mt019Block4.setBlock4108Mur("ABC123");
        mt019Block4.setBlock4102SwiftAddress("SWIFT123");
        mt019Block4.setBlock4107Mor("MORValue");
        mt019Block4.setBlock4432AbortReason("AbortReason");
        mt019Block4.setBlock4619VasCode("VASCode");

        // Asserting values
        assertEquals("10:30", mt019Block4.getBlock4175InputTime());
        assertEquals("123456", mt019Block4.getBlock4106Mir());
        assertEquals("ABC123", mt019Block4.getBlock4108Mur());
        assertEquals("SWIFT123", mt019Block4.getBlock4102SwiftAddress());
        assertEquals("MORValue", mt019Block4.getBlock4107Mor());
        assertEquals("AbortReason", mt019Block4.getBlock4432AbortReason());
        assertEquals("VASCode", mt019Block4.getBlock4619VasCode());
    }

    @Test
    public void testToString() {
        // Creating a new MT019Block4 object
        MT019Block4 mt019Block4 = new MT019Block4();
        mt019Block4.setBlock4175InputTime("10:30");
        mt019Block4.setBlock4106Mir("123456");
        mt019Block4.setBlock4108Mur("ABC123");
        mt019Block4.setBlock4102SwiftAddress("SWIFT123");
        mt019Block4.setBlock4107Mor("MORValue");
        mt019Block4.setBlock4432AbortReason("AbortReason");
        mt019Block4.setBlock4619VasCode("VASCode");

        // Asserting the toString output
        String expectedString = "MT019Block4 [block4175InputTime=10:30, block4106Mir=123456, block4108Mur=ABC123, block4102SwiftAddress=SWIFT123, block4107Mor=MORValue, block4432AbortReason=AbortReason, block4619VasCode=VASCode]";
        assertEquals(expectedString, mt019Block4.toString());
    }
}
