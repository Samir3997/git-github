package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import java.math.BigDecimal;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag99ATest {

    @Test
    public void testBlock4Tag99A() {
        // Creating a new Block4Tag99A object
        Block4Tag99A block4Tag99A = new Block4Tag99A();
        
        // Setting values
        block4Tag99A.setBlock4Tag99AQualifier("QualifierA");
        block4Tag99A.setBlock4Tag99ASign("+");
        block4Tag99A.setBlock4Tag99AAmount(new BigDecimal("1000.00"));

        // Asserting values
        assertEquals("QualifierA", block4Tag99A.getBlock4Tag99AQualifier());
        assertEquals("+", block4Tag99A.getBlock4Tag99ASign());
        assertEquals(new BigDecimal("1000.00"), block4Tag99A.getBlock4Tag99AAmount());
    }
}
