package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag57Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag57 block4Tag57 = new Block4Tag57();

        // Set properties
        block4Tag57.setBlock4Tag57aDcmark("DCMarkA");
        block4Tag57.setBlock4Tag57aAcct("accountA");
        block4Tag57.setBlock4Tag57aBic("BIC_A");
        block4Tag57.setBlock4Tag57bDcmark("DCMarkB");
        block4Tag57.setBlock4Tag57bAcct("accountB");
        block4Tag57.setBlock4Tag57bLocation("Location B");
        block4Tag57.setBlock4Tag57dDcmark("DCMarkD");
        block4Tag57.setBlock4Tag57dAcct("accountD");
        block4Tag57.setBlock4Tag57dNameAddr("Name Address D");

        // Assert properties
        assertEquals("DCMarkA", block4Tag57.getBlock4Tag57aDcmark());
        assertEquals("accountA", block4Tag57.getBlock4Tag57aAcct());
        assertEquals("BIC_A", block4Tag57.getBlock4Tag57aBic());
        assertEquals("DCMarkB", block4Tag57.getBlock4Tag57bDcmark());
        assertEquals("accountB", block4Tag57.getBlock4Tag57bAcct());
        assertEquals("Location B", block4Tag57.getBlock4Tag57bLocation());
        assertEquals("DCMarkD", block4Tag57.getBlock4Tag57dDcmark());
        assertEquals("accountD", block4Tag57.getBlock4Tag57dAcct());
        assertEquals("Name Address D", block4Tag57.getBlock4Tag57dNameAddr());
    }
}
