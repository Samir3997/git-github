import static org.junit.jupiter.api.Assertions.assertEquals;

import java.math.BigDecimal;

import org.junit.jupiter.api.Test;

public class MT200Block4Test {

    private static final long EXPECTED_SERIAL_VERSION_UID = -3402887972982015083L;

    @Test
    public void testMT200Block4() {
        // Create an instance of MT200Block4
        MT200Block4 block4 = new MT200Block4();

        // Set values
        block4.setBlock4Tag20("TAG20_VALUE");
        block4.setBlock4Tag32ADate("2024-11-01");
        block4.setBlock4Tag32ACurr("USD");
        block4.setBlock4Tag32AAmnt(BigDecimal.valueOf(1000.00));

        // Assert values
        assertEquals("TAG20_VALUE", block4.getBlock4Tag20());
        assertEquals("2024-11-01", block4.getBlock4Tag32ADate());
        assertEquals("USD", block4.getBlock4Tag32ACurr());
        assertEquals(BigDecimal.valueOf(1000.00), block4.getBlock4Tag32AAmnt());
    }

    @Test
    public void testToString() {
        // Create an instance of MT200Block4
        MT200Block4 block4 = new MT200Block4();
        block4.setBlock4Tag20("TAG20_VALUE");
        block4.setBlock4Tag32ADate("2024-11-01");
        block4.setBlock4Tag32ACurr("USD");
        block4.setBlock4Tag32AAmnt(BigDecimal.valueOf(1000.00));

        // Assert the toString output
        String expectedString = "MT200Block4 [block4Tag20=TAG20_VALUE, block4Tag32ADate=2024-11-01, "
                              + "block4Tag32ACurr=USD, block4Tag32AAmnt=1000.00]";
        assertEquals(expectedString, block4.toString());
    }

    @Test
    public void testSerialVersionUID() {
        // Assert that the serialVersionUID is as expected
        assertEquals(EXPECTED_SERIAL_VERSION_UID, MT200Block4.getSerialversionuid());
    }
}
