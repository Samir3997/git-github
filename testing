package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class MT012Block4Test {

    @Test
    public void testMT012Block4() {
        // Creating a new MT012Block4 object
        MT012Block4 mt012Block4 = new MT012Block4();
        
        // Setting values
        mt012Block4.setBlock4175InputTime("10:30");
        mt012Block4.setBlock4106Mir("123456");
        mt012Block4.setBlock4108Mur("ABC123");
        mt012Block4.setBlock4102SwiftAddress("SWIFT123");
        mt012Block4.setBlock4103ServiceCode("SERVICE01");
        mt012Block4.setBlock4114PaymentReleaseInformationSender("SenderInfo");

        // Asserting values
        assertEquals("10:30", mt012Block4.getBlock4175InputTime());
        assertEquals("123456", mt012Block4.getBlock4106Mir());
        assertEquals("ABC123", mt012Block4.getBlock4108Mur());
        assertEquals("SWIFT123", mt012Block4.getBlock4102SwiftAddress());
        assertEquals("SERVICE01", mt012Block4.getBlock4103ServiceCode());
        assertEquals("SenderInfo", mt012Block4.getBlock4114PaymentReleaseInformationSender());
    }

    @Test
    public void testToString() {
        // Creating a new MT012Block4 object
        MT012Block4 mt012Block4 = new MT012Block4();
        mt012Block4.setBlock4175InputTime("10:30");
        mt012Block4.setBlock4106Mir("123456");
        mt012Block4.setBlock4108Mur("ABC123");
        mt012Block4.setBlock4102SwiftAddress("SWIFT123");
        mt012Block4.setBlock4103ServiceCode("SERVICE01");
        mt012Block4.setBlock4114PaymentReleaseInformationSender("SenderInfo");

        // Asserting the toString output
        String expectedString = "MT012Block4 [block4175InputTime=10:30, block4106Mir=123456, block4108Mur=ABC123, block4102SwiftAddress=SWIFT123, block4103ServiceCode=SERVICE01, block4114PaymentReleaseInformationSender=SenderInfo]";
        assertEquals(expectedString, mt012Block4.toString());
    }
}
