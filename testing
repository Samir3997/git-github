package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import java.math.BigDecimal;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag90Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag90 block4Tag90 = new Block4Tag90();

        // Set properties
        block4Tag90.setBlock4Tag90AQualifier("QualifierA");
        block4Tag90.setBlock4Tag90APercCode("PERC001");
        block4Tag90.setBlock4Tag90ASign("+");
        block4Tag90.setBlock4Tag90APrice(BigDecimal.valueOf(100.50));
        block4Tag90.setBlock4Tag90BQualifier("QualifierB");
        block4Tag90.setBlock4Tag90BAmntCode("AMNT001");
        block4Tag90.setBlock4Tag90BCurrency("USD");
        block4Tag90.setBlock4Tag90BPrice(BigDecimal.valueOf(200.75));

        // Assert properties
        assertEquals("QualifierA", block4Tag90.getBlock4Tag90AQualifier());
        assertEquals("PERC001", block4Tag90.getBlock4Tag90APercCode());
        assertEquals("+", block4Tag90.getBlock4Tag90ASign());
        assertEquals(BigDecimal.valueOf(100.50), block4Tag90.getBlock4Tag90APrice());
        assertEquals("QualifierB", block4Tag90.getBlock4Tag90BQualifier());
        assertEquals("AMNT001", block4Tag90.getBlock4Tag90BAmntCode());
        assertEquals("USD", block4Tag90.getBlock4Tag90BCurrency());
        assertEquals(BigDecimal.valueOf(200.75), block4Tag90.getBlock4Tag90BPrice());
    }
}
