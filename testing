package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag59Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag59 block4Tag59 = new Block4Tag59();

        // Set properties
        block4Tag59.setBlock4Tag59Acct("account59");
        block4Tag59.setBlock4Tag59NameAddr("Name Address 59");
        block4Tag59.setBlock4Tag59aAcct("account59A");
        block4Tag59.setBlock4Tag59aBic("BIC59A");
        block4Tag59.setBlock4Tag59fAcct("account59F");
        block4Tag59.setBlock4Tag59fNumber1("Number1");
        block4Tag59.setBlock4Tag59fNameAddr1("Name Addr 1");
        block4Tag59.setBlock4Tag59fNumber2("Number2");
        block4Tag59.setBlock4Tag59fNameAddr2("Name Addr 2");
        block4Tag59.setBlock4Tag59fNumber3("Number3");
        block4Tag59.setBlock4Tag59fNameAddr3("Name Addr 3");
        block4Tag59.setBlock4Tag59fNumber4("Number4");
        block4Tag59.setBlock4Tag59fNameAddr4("Name Addr 4");

        // Assert properties
        assertEquals("account59", block4Tag59.getBlock4Tag59Acct());
        assertEquals("Name Address 59", block4Tag59.getBlock4Tag59NameAddr());
        assertEquals("account59A", block4Tag59.getBlock4Tag59aAcct());
        assertEquals("BIC59A", block4Tag59.getBlock4Tag59aBic());
        assertEquals("account59F", block4Tag59.getBlock4Tag59fAcct());
        assertEquals("Number1", block4Tag59.getBlock4Tag59fNumber1());
        assertEquals("Name Addr 1", block4Tag59.getBlock4Tag59fNameAddr1());
        assertEquals("Number2", block4Tag59.getBlock4Tag59fNumber2());
        assertEquals("Name Addr 2", block4Tag59.getBlock4Tag59fNameAddr2());
        assertEquals("Number3", block4Tag59.getBlock4Tag59fNumber3());
        assertEquals("Name Addr 3", block4Tag59.getBlock4Tag59fNameAddr3());
        assertEquals("Number4", block4Tag59.getBlock4Tag59fNumber4());
        assertEquals("Name Addr 4", block4Tag59.getBlock4Tag59fNameAddr4());
    }
}
