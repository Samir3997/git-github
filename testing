package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag55Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag55 block4Tag55 = new Block4Tag55();

        // Set properties
        block4Tag55.setBlock4Tag55aDcmark("DCMarkA");
        block4Tag55.setBlock4Tag55aAcct("accountA");
        block4Tag55.setBlock4Tag55aBic("BIC_A");
        block4Tag55.setBlock4Tag55bDcmark("DCMarkB");
        block4Tag55.setBlock4Tag55bAcct("accountB");
        block4Tag55.setBlock4Tag55bLocation("Location B");
        block4Tag55.setBlock4Tag55dDcmark("DCMarkD");
        block4Tag55.setBlock4Tag55dAcct("accountD");
        block4Tag55.setBlock4Tag55dNameAddr("Name Address D");

        // Assert properties
        assertEquals("DCMarkA", block4Tag55.getBlock4Tag55aDcmark());
        assertEquals("accountA", block4Tag55.getBlock4Tag55aAcct());
        assertEquals("BIC_A", block4Tag55.getBlock4Tag55aBic());
        assertEquals("DCMarkB", block4Tag55.getBlock4Tag55bDcmark());
        assertEquals("accountB", block4Tag55.getBlock4Tag55bAcct());
        assertEquals("Location B", block4Tag55.getBlock4Tag55bLocation());
        assertEquals("DCMarkD", block4Tag55.getBlock4Tag55dDcmark());
        assertEquals("accountD", block4Tag55.getBlock4Tag55dAcct());
        assertEquals("Name Address D", block4Tag55.getBlock4Tag55dNameAddr());
    }
}
