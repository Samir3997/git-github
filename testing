import static org.junit.jupiter.api.Assertions.assertEquals;

import java.math.BigDecimal;

import org.junit.jupiter.api.Test;

public class MT103Block4Test {

    @Test
    public void testMT103Block4() {
        // Create an instance of MT103Block4
        MT103Block4 block4 = new MT103Block4();
        
        // Set values
        block4.setBlock4Tag20("TAG20Value");
        block4.setBlock4Tag23b("TAG23BValue");
        block4.setBlock4Tag26t("TAG26TValue");
        block4.setBlock4Tag32aDate("2023-01-01");
        block4.setBlock4Tag32aCurrency("USD");
        block4.setBlock4Tag32aSettledAmnt(new BigDecimal("1000.00"));
        block4.setBlock4Tag33bCurrency("EUR");
        block4.setBlock4Tag33bInstrucAmnt(new BigDecimal("500.00"));
        block4.setBlock4Tag36("TAG36Value");
        block4.setBlock4Tag51aDcmark("DCMarkValue");
        block4.setBlock4Tag51aAcct("AccountValue");
        block4.setBlock4Tag51aBic("BICValue");
        block4.setBlock4Tag56c("TAG56CValue");
        block4.setBlock4Tag57c("TAG57CValue");

        // Assert values
        assertEquals("TAG20Value", block4.getBlock4Tag20());
        assertEquals("TAG23BValue", block4.getBlock4Tag23b());
        assertEquals("TAG26TValue", block4.getBlock4Tag26t());
        assertEquals("2023-01-01", block4.getBlock4Tag32aDate());
        assertEquals("USD", block4.getBlock4Tag32aCurrency());
        assertEquals(new BigDecimal("1000.00"), block4.getBlock4Tag32aSettledAmnt());
        assertEquals("EUR", block4.getBlock4Tag33bCurrency());
        assertEquals(new BigDecimal("500.00"), block4.getBlock4Tag33bInstrucAmnt());
        assertEquals("TAG36Value", block4.getBlock4Tag36());
        assertEquals("DCMarkValue", block4.getBlock4Tag51aDcmark());
        assertEquals("AccountValue", block4.getBlock4Tag51aAcct());
        assertEquals("BICValue", block4.getBlock4Tag51aBic());
        assertEquals("TAG56CValue", block4.getBlock4Tag56c());
        assertEquals("TAG57CValue", block4.getBlock4Tag57c());
    }
}
