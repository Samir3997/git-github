package com.maybank.ilm.embeddable;

import org.junit.jupiter.api.Test;

import static org.junit.jupiter.api.Assertions.assertEquals;

public class Block4Tag56Test {

    @Test
    public void testSetAndGetProperties() {
        Block4Tag56 block4Tag56 = new Block4Tag56();

        // Set properties
        block4Tag56.setBlock4Tag56aDcmark("DCMarkA");
        block4Tag56.setBlock4Tag56aAcct("accountA");
        block4Tag56.setBlock4Tag56aBic("BIC_A");
        block4Tag56.setBlock4Tag56dDcmark("DCMarkD");
        block4Tag56.setBlock4Tag56dAcct("accountD");
        block4Tag56.setBlock4Tag56dNameAddr("Name Address D");

        // Assert properties
        assertEquals("DCMarkA", block4Tag56.getBlock4Tag56aDcmark());
        assertEquals("accountA", block4Tag56.getBlock4Tag56aAcct());
        assertEquals("BIC_A", block4Tag56.getBlock4Tag56aBic());
        assertEquals("DCMarkD", block4Tag56.getBlock4Tag56dDcmark());
        assertEquals("accountD", block4Tag56.getBlock4Tag56dAcct());
        assertEquals("Name Address D", block4Tag56.getBlock4Tag56dNameAddr());
    }
}
